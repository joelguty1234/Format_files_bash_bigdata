{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        { "src": "/views/upload.ejs", "dest": "/views/upload.ejs" },
        { "src": "/views/style.css", "dest": "/views/style.css" }
    ]
}